     {
     "Version": "2012-10-17",
     "Id": "PolicyForCloudFrontPrivateContent",
     "Statement": [
         {
             "Sid": "1",
             "Effect": "Allow",
             "Principal": {
                 "AWS": "arn:aws:iam::cloudfront:user/CloudFront Origin Access Identity ID"
             },
             "Action": "s3:Get*",
             "Resource": "arn:aws:s3:::bucket_name/*"
         }
     ]
